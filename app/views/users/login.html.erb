<nav>
  <ul>
  <% if @session == nil %>
  <%=link_to "Home", login_path%>
  <% else %>
  <%= link_to "Home", user_path(@session)%>
  <%end%>
    <li>
      <a href="/studios">Studios</a>
    </li>
    <li>
      <a href="/engineers">Engineers</a>
    </li>
    <li>
    <a href="/logout">Log Out</a>
    </li>
  </ul>
</nav>

<%if flash[:message]%>
    <%flash[:message].each do |e|%>
        <p><%=e%></p>
     <%end%>
<%end%>
<h1>LOGIN</h1>
<br>
<%= form_tag "/login" do  %>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
    <br>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <br>
    <br>
    <%= submit_tag "Log in!" %>
<% end %>
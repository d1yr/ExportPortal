<h1>LOGIN</h1>
<br>
<%= form_tag "/login" do  %>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
    <br>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
    <br>
    <br>
    <%= submit_tag "Log in!", class:'button' %>
<% end %>
<p> Not a member?</p>
<a href="/users/new">SIGN UP</a>